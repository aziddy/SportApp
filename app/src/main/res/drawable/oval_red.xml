<layer-list xmlns:android="http://schemas.android.com/apk/res/android">



    <item android:id="@+id/fabShape">
        <shape android:shape="oval">
            <solid android:color="#ff0004" />
        </shape>
    </item>

    <item
        android:left="5dp"
        android:right="5dp">
            <shape
                android:shape="line">
                <stroke

                    android:width="4dp"
                    android:color="#ffffff"
                    android:dashWidth="1dp"
                    android:dashGap="2dp" />
            </shape>

    </item>

</layer-list>