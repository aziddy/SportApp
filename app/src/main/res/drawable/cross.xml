<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>

        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">

            <solid
                android:color="#30ff01"/>

            <size
                android:width="120dp"
                android:height="120dp"/>
        </shape>

    </item>







    <item
        android:left="15dp"
        android:right="15dp"
        >
<rotate
    android:fromDegrees="90"
    android:toDegrees="90"
    >
    <shape
        android:shape="line">
        <stroke

            android:width="4dp"
            android:color="#ffffff"
            android:dashWidth="1dp"
            android:dashGap="2dp" />
    </shape>
</rotate>
</item>

    <item
        android:left="15dp"
        android:right="15dp"
        >
            <shape
                android:shape="line">
                <stroke

                    android:width="4dp"
                    android:color="#ffffff"
                    android:dashWidth="1dp"
                    android:dashGap="2dp" />
            </shape>
    </item>

</layer-list>